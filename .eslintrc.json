{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb", "airbnb/hooks"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "import/no-extraneous-dependencies": "off",
        "linebreak-style": "off",
        "react/require-default-props": "off",
        "react/function-component-definition": [2, {
          "namedComponents": "arrow-function"
        }],
        "arrow-parens": ["error", "as-needed"],
        "no-param-reassign": 0
    }
}
